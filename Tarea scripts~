
(defun area-circulo2()
  (terpri)
  (princ "Por favor, teclea un radio ")
  (setq radio (read))
  (format t "El area del circulo con radio ~4F es: ~7F~%" radio (* pi radio radio))
)



(defun area-rectangulo()
  (princ "Por favor ingrese el valor de la base: ")
  (setq base (read))
  (princ "Por favor ingrese el valor de la altura: ")
  (setq altura (read))
  (formar t "El area del rectangulo es: ~ 3F~ " (* base altura))
)